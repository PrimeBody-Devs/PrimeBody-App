{
  "name": "primebody-landing",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:cloudflare": "next build",
    "start": "next start",
    "deploy:cloudflare": "npm run build:cloudflare",
    "preview:cloudflare": "npx wrangler pages dev .next",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out dist",
    "analyze": "ANALYZE=true npm run build",
    "build:analyze": "npm run clean && ANALYZE=true npm run build",
    "lighthouse": "lhci autorun",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "precommit": "npm run lint && npm run type-check",
    "prebuild": "npm run clean && npm run type-check",
    "optimize": "node scripts/optimize-for-production.js",
    "deploy:check": "npm run optimize",
    "content:validate": "node scripts/content-manager.js validate",
    "content:check-env": "node scripts/content-manager.js check-env",
    "content:stats": "node scripts/content-manager.js stats",
    "content:health": "node scripts/content-manager.js health-check",
    "content:template": "node scripts/content-manager.js generate-template",
    "content:env-template": "node scripts/content-manager.js generate-env",
    "optimize": "node scripts/optimize-for-production.js",
    "optimize:env": "node scripts/optimize-for-production.js env",
    "optimize:build": "node scripts/optimize-for-production.js build",
    "optimize:bundle": "node scripts/optimize-for-production.js bundle",
    "optimize:vitals": "node scripts/optimize-for-production.js vitals",
    "optimize:images": "node scripts/optimize-for-production.js images",
    "optimize:a11y": "node scripts/optimize-for-production.js a11y",
    "optimize:seo": "node scripts/optimize-for-production.js seo",
    "optimize:security": "node scripts/optimize-for-production.js security",
    "cross-browser": "node scripts/cross-browser-test.js",
    "lighthouse:ci": "lhci autorun --config=lighthouse.config.js",
    "lighthouse:desktop": "lighthouse http://localhost:3000 --preset=desktop --output=html --output-path=./lighthouse-desktop.html",
    "lighthouse:mobile": "lighthouse http://localhost:3000 --preset=mobile --output=html --output-path=./lighthouse-mobile.html",
    "deploy:check": "npm run optimize && npm run test && npm run lighthouse:ci",
    "deploy:checklist": "node scripts/generate-deployment-checklist.js",
    "deploy:ready": "npm run deploy:checklist && npm run optimize && npm run test"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.0",
    "@neynar/nodejs-sdk": "^1.71.0",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-query-devtools": "^5.83.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.7",
    "lucide-react": "^0.525.0",
    "next": "^15.4.4",
    "next-themes": "^0.4.6",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.61.1",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "viem": "^2.33.0",
    "wagmi": "^2.16.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.15",
    "@lhci/cli": "^0.13.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.9",
    "@types/react": "18.3.23",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.30",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lighthouse": "^11.4.0",
    "pino-pretty": "^13.0.0",
    "postcss": "^8",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "puppeteer": "^21.6.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "web-vitals": "^3.5.0",
    "webpack-bundle-analyzer": "^4.10.1"
  }
}